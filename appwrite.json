{
    "projectId": "mail-memo",
    "projectName": "Mail Memo",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": false,
            "health": false,
            "storage": true,
            "teams": false,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": false,
                "phone": false,
                "invites": false,
                "anonymous": false,
                "email-otp": false,
                "magic-url": false,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false
            }
        }
    },
    "functions": [
        {
            "$id": "sendMails",
            "name": "sendMails",
            "runtime": "go-1.22",
            "execute": [
                "users"
            ],
            "events": [],
            "schedule": "",
            "timeout": 60,
            "enabled": true,
            "logging": true,
            "entrypoint": "main.go",
            "commands": "",
            "path": "functions/sendMails",
            "scopes": [
                "users.read",
                "documents.read",
                "execution.write"
            ]
        },
        {
            "$id": "syncCalendar",
            "name": "syncCalendar",
            "runtime": "go-1.22",
            "execute": [],
            "events": [],
            "schedule": "",
            "timeout": 900,
            "enabled": true,
            "logging": true,
            "entrypoint": "main.go",
            "commands": "",
            "path": "functions/syncCalendar",
            "scopes": [
                "documents.read",
                "documents.write",
                "users.read"
            ]
        },
        {
            "$id": "syncCalendarScheduler",
            "name": "syncCalendarScheduler",
            "runtime": "go-1.22",
            "execute": [
                "users"
            ],
            "events": [],
            "schedule": "*/15 * * * *",
            "timeout": 900,
            "enabled": true,
            "logging": true,
            "entrypoint": "main.go",
            "commands": "",
            "path": "functions/syncCalendarScheduler",
            "scopes": [
                "documents.read",
                "execution.write"
            ]
        },
        {
            "$id": "sendMailsScheduler",
            "name": "sendMailsScheduler",
            "runtime": "go-1.22",
            "execute": [],
            "events": [],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "main.go",
            "commands": "",
            "path": "functions/sendMailsScheduler",
            "scopes": [
                "users.read",
                "execution.write"
            ]
        },
        {
            "$id": "api",
            "name": "api",
            "runtime": "go-1.22",
            "execute": [
                "users"
            ],
            "scopes": [
                "users.write",
                "documents.write",
                "users.read"
            ],
            "events": [],
            "schedule": "",
            "timeout": 15,
            "enabled": true,
            "logging": true,
            "entrypoint": "main.go",
            "commands": "",
            "path": "functions/api"
        },
        {
            "$id": "nodeSendMailInternal",
            "name": "nodeSendMailInternal",
            "runtime": "node-18.0",
            "execute": [],
            "events": [],
            "scopes": ["messages.write"],
            "schedule": "",
            "timeout": 30,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/main.js",
            "commands": "npm install",
            "path": "functions/nodeSendMailInternal"
        }
    ],
    "databases": [
        {
            "$id": "main",
            "name": "main",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "calendars",
            "$permissions": [
                "create(\"users\")"
            ],
            "databaseId": "main",
            "name": "calendars",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "url",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 2048,
                    "default": null
                },
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "color",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 7,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "index_1",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "index_2",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId",
                        "$id"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "events",
            "$permissions": [],
            "databaseId": "main",
            "name": "events",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "calendarId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "name",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "uid",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "modifiedAt",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "startAt",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "endAt",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "index_1",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "calendarId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                },
                {
                    "key": "index_2",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "calendarId",
                        "startAt",
                        "endAt"
                    ],
                    "orders": [
                        "ASC",
                        "ASC",
                        "ASC"
                    ]
                },
                {
                    "key": "index_3",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "calendarId",
                        "uid"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                }
            ]
        }
    ],
    "buckets": [
        {
            "$id": "email-assets",
            "$permissions": [
                "read(\"any\")"
            ],
            "fileSecurity": false,
            "name": "Email assets",
            "enabled": true,
            "maximumFileSize": 5000000,
            "allowedFileExtensions": [
                "png",
                "jpg",
                "gif"
            ],
            "compression": "none",
            "encryption": true,
            "antivirus": true
        }
    ]
}